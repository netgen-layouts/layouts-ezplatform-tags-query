{
    "name": "netgen/layouts-tags-query",
    "description": "Netgen Layouts tags query",
    "license": "proprietary",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "https://www.netgenlabs.com"
        }
    ],
    "require": {
        "netgen/layouts-ezplatform": "^1.0",
        "netgen/tagsbundle": "^3.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "vimeo/psalm": "^3.4"
    },
    "scripts": {
        "phpstan": "@php vendor/bin/phpstan analyse -c phpstan.neon --level=7 --ansi lib bundle",
        "psalm": "@php vendor/bin/psalm -c psalm.xml --show-info=false"
    },
    "repositories": [
        { "type": "composer", "url": "https://packagist.netgen.biz" }
    ],
    "autoload": {
        "psr-4": {
            "Netgen\\Layouts\\TagsQuery\\": "lib/",
            "Netgen\\Bundle\\LayoutsTagsQueryBundle\\": "bundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
